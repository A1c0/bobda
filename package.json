{
  "name": "bobda",
  "version": "0.3.0",
  "description": "Some tools for ramda",
  "main": "bobda.js",
  "repository": {
    "url": "https://github.com/A1c0/bobda",
    "type": "git"
  },
  "license": "MIT",
  "keywords": [
    "ramda",
    "utils",
    "utilities",
    "toolkit",
    "extensions",
    "addons",
    "adjunct",
    "recipes",
    "extras",
    "cookbook",
    "functional"
  ],
  "homepage": "https://github.com/A1c0/bobda",
  "bugs": "https://github.com/A1c0/bobda/issues",
  "contributors": [
    "Baptiste Chevallier <baptiste@thechatbotfactory.com>",
    "Alexandre Contassot <alexandrecontassot@hotmail.fr>"
  ],
  "files": [
    "bobda.js",
    "app/*.js"
  ],
  "scripts": {
    "test": "nyc mocha --recursive --parallel",
    "code-style": "xo"
  },
  "pre-commit": [
    "code-style",
    "test"
  ],
  "xo": {
    "envs": [
      "mocha"
    ],
    "space": 2,
    "prettier": true,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": 0
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "deep-equal-in-any-order": "^1.0.13",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "sanctuary-maybe": "^2.1.0"
  }
}
